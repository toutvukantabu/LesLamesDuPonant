<form method="post" action="{{ path('time_stamp_delete', {'id': time_stamp.id}) }}" onsubmit="return confirm('Confirmer la suppression?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ time_stamp.id) }}">
    <button class="btn">Supprimer</button>
</form>
